<!-- Pregunta en plural -> Respuesta en singular {{{3 -->
<category>
	<pattern>^ cuales ^ son [sus tus] ^ defectos</pattern>
	<template><random>
		<li>Pues se</li>
		<li>Se</li>
	</random> me ocure <random>
		<li>uno</li>
		<li>solo uno</li>
	</random>. <srai>cual es tu mayor defecto</srai></template>
</category>
